{
  "name": "tour",
  "version": "3.0.0",
  "description": "A simple, minimalist touring and on-boarding library for javascript",
  "main": "dist/tour.js",
  "files": [
    "example",
    "dist",
    "src",
    "tour.*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:example": "parcel serve example/index.html --no-hmr",
    "build:example": "parcel build example/index.html",
    "watch:lib": "parcel serve src/tour.js",
    "build:lib": "parcel build src/tour.js",
    "clean": "rm -r dist && mkdir dist",
    "prepublish": "npm run build:lib && npm run build:example"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/tourjs/tour.git"
  },
  "author": "Tanner Linsley <tannerlinsley@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tourjs/tour/issues"
  },
  "homepage": "https://github.com/tourjs/tour",
  "dependencies": {
    "TinyAnimate": "^0.3.0",
    "cash-dom": "^1.3.4"
  },
  "devDependencies": {
    "parcel": "^1.9.4",
    "stylus": "^0.54.5"
  }
}
